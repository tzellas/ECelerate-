      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "packages": {},
        "requests": {},
        "exec": [
          "pm.collectionVariables.set(\"id\", \"1\");\r",
          "\r",
          "function formatDateTime(d) {\r",
          "  const year = d.getFullYear();\r",
          "  const month = String(d.getMonth() + 1).padStart(2, '0');\r",
          "  const day = String(d.getDate()).padStart(2, '0');\r",
          "  const hours = String(d.getHours()).padStart(2, '0');\r",
          "  const mins = String(d.getMinutes()).padStart(2, '0');\r",
          "  return `${year}-${month}-${day} ${hours}:${mins}`;\r",
          "}\r",
          "\r",
          "function yyyymmdd(d) {\r",
          "  const year = d.getFullYear();\r",
          "  const month = String(d.getMonth() + 1).padStart(2, '0');\r",
          "  const day = String(d.getDate()).padStart(2, '0');\r",
          "  return `${year}${month}${day}`;\r",
          "}\r",
          "\r",
          "const now = new Date();\r",
          "\r",
          "const S1 = new Date(now.getTime() + 30 * 60 * 1000); \r",
          "const E1 = new Date(S1.getTime() + 40 * 60 * 1000); \r",
          "const S2 = new Date(E1.getTime() + 1 * 60 * 1000);  \r",
          "const E2 = new Date(S2.getTime() + 40 * 60 * 1000);\r",
          "\r",
          "pm.collectionVariables.set(\"S1\", formatDateTime(S1));\r",
          "pm.collectionVariables.set(\"E1\", formatDateTime(E1));\r",
          "pm.collectionVariables.set(\"S2\", formatDateTime(S2));\r",
          "pm.collectionVariables.set(\"E2\", formatDateTime(E2));\r",
          "\r",
          "// date only\r",
          "pm.collectionVariables.set(\"S1_DATE\", yyyymmdd(S1));\r",
          "pm.collectionVariables.set(\"E1_DATE\", yyyymmdd(E1));\r",
          "pm.collectionVariables.set(\"S2_DATE\", yyyymmdd(S2));\r",
          "pm.collectionVariables.set(\"E2_DATE\", yyyymmdd(E2));\r",
          "\r",
          ""
        ]
      }
